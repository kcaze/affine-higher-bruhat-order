    "from lib import *\n",
    "\n",
    "def BBalgorithm(P):\n",
    "    n = len(P)+1\n",
    "    w = list(range(1,n+1))\n",
    "    idxs = []\n",
    "    for i in range(n-2,-1,-1):\n",
    "        idxs += [i+2]*P[i]\n",
    "    for i in idxs:\n",
    "        w = E(w, i)\n",
    "    return w\n",
    "\n",
    "print(BBalgorithm([1,2,4]))"
   ]
  }
